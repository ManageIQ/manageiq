#results_tree_div
  - if @results
    %br
    #treebox{:style => "width: auto;"}
    = render(:partial => "layouts/dynatree",
      :locals         => {:tree_id   => "treebox",
        :json_tree                   => @json_tree,
        :multi_lines                 => true,
        :exp_tree                    => true,
        :open_close_all_on_dbl_click => true})

  - else
    = render :partial => 'layouts/info_msg',
      :locals => {:message => _("Enter Automation Simulation options on the left and press Submit.")}
