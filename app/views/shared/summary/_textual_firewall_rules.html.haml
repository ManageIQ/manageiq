- if items
  %table.table.table-bordered.table-striped
    %thead
      %tr
        %th{:colspan => 6, :align => "left"}
          = title
    - labels.each do |label|
      %td
        %strong
          = label
    - items.each do |item|
      %tr
        - item.each do |value|
          %td
            = value
