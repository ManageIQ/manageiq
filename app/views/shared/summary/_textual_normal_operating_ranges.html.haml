- if items
  %table.table.table-bordered.table-striped
    %thead
      %tr
        %th{:colspan => "3"}
          = title
    - items.each do |item|
      %tr
        %td{:rowspan => item[:value].length+1}
          = item[:label]
        %th
          &nbsp;
        %th
          &nbsp;
      - item[:value].each do |subitem|
        %tr
          %td
            = subitem[:label]
          %td
            = subitem[:value]