%div{:id => "group_#{group.id}_div"}
  %h3{:title => "#{group.description}"}
    = group.label
  - unless group.dialog_fields.empty?
    .form-horizontal
      - group.dialog_fields.select(&:visible).each do |field|
        = render :partial => "shared/dialogs/dialog_field",
                 :locals  => {:wf => wf, :field => field}
  %hr
