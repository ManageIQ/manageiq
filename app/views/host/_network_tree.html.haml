-# Network or Storage Adpater Tree
%div{:id => "#{session[:tree_name]}box", :style => "width: 100%"}
  = render(:partial => "layouts/dynatree",
           :locals  => {:tree_id                     => "#{session[:tree_name]}box",
                        :tree_name                   => session[:tree_name],
                        :json_tree                   => instance_variable_get(:"@#{session[:tree_name]}"),
                        :click_url                   => "/vm/show/",
                        :onmousein                   => "miqOnMouseInHostNet",
                        :onmouseout                  => "miqOnMouseOutHostNet",
                        :onclick                     => "miqOnClickHostNet",
                        :exp_tree                    => true,
                        :open_close_all_on_dbl_click => true})

