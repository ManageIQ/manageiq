- # Div to hold the actual tree
#condition_tree_div{:style => "width: 100%; height: 100%"}
  #condition_treebox{:style => "width: 100%; height: 100%; color: #000; overflow: hidden;"}
  = render(:partial => "layouts/dynatree",
    :locals => {:tree_id => "condition_treebox",
      :tree_name         => "condition_tree",
      :id_prefix         => "co_",
      :json_tree         => @temp[:condition_tree],
      :onclick           => "cfmeOnClick_SelectTreeNode",
      :select_node       => "#{x_node}",
      :base_id           => "root",
      :no_base_exp       => true,
      :exp_tree          => false,
      :highlighting      => true,
      :tree_state        => true,
      :autoload          => true,
      :multi_lines       => true})
