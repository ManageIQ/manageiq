- if items
  %table.table.table-bordered.table-striped
    %thead
      %tr
        %th{:colspan => 6, :align => "left"}
          = title
    - items[:labels].each do |label|
      %td
        %strong
          = label
    - items[:values].each do |item|
      %tr
        - item.each do |value|
          - if value.is_a?(Hash)
            %td{:title => value[:title]}
              = value[:text]
          - else
            %td
              = value
