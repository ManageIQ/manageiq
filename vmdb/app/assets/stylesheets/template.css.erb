legend { display: none;}
a { text-decoration: none;}
h2 { margin:0; padding:0;line-height: 1.4em;font-family: OpenSansSemibold,Arial,Helvetica,sans-serif; font-weight:normal}
img { border:0;}
textarea, input, select {font-family: OpenSansRegular,Arial,Helvetica,sans-serif;font-size: 12px}
textarea { width: 98.5%; border: 1px solid #d1d1d1; padding: 7px; line-height: 1.5em}
