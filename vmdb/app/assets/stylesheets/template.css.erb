/* begin modified Patternfly code */

.btn-disabled {
  opacity:0.4; 
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)"; 
  filter:alpha(opacity=40);
  filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=40);
  cursor: default;
}

/* end modified Patternfly code */

fieldset { 
  position: relative; 
  z-index: 0;  
  margin: 0 7px 20px 0px;
  padding: 10px;
  border: 1px solid #e9e8e8; 
  background-color: #fff;
}

fieldset h3 { 
  position: relative;
  margin: 0 0 10px 0;
  padding: 5px 0 10px 0;
  color:#4b4b4b;
  text-align: left;
  font: normal 12px OpenSansSemibold,Arial,Helvetica,sans-serif;
}

nobr { white-space: nowrap;}

/*Centering for Hovered VMs*/
#outer { position: absolute;top: 50%;left: 0;overflow: visible;width: 100%;height: 1px;}
#inner {position: absolute;/***  height / 2   ***/ top: -50px;left: 50%;margin-left: -50px;width: 100px;height: 100px;  /***  width / 2  ***/}

#notification, #search_notification {
  position:fixed;
  bottom:0;
  z-index:1000;
  display: block;
  margin:0;
  padding:0;
  width: 100%; 
  height:100%;
  background: url(/images/layout/overlay_dark.png);
  color: #fff;
  text-align:center;
  text-shadow: #000 2px 2px 0;
  font: normal 116% OpenSansSemibold,Arial,Helvetica,sans-serif;
}

#blocker_div { 
  position: fixed;
  bottom: 0;
  z-index:48; 
  display: block; 
  margin: 0;  
  padding: 100px 0 0 0;
  width: 100%;
  height: 100%;
  background: url(/images/layout/overlay_dark.png); 
  color: #fff;
  text-shadow: #000 2px 2px 0;
}

/********************** Task Bar **********************/

#taskbar_div  { 
  z-index: 99;
  width: auto;   
  height: 32px;
  background-image: linear-gradient(to bottom, #FAFAFA 0%, #EDEDED 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fffafafa', endColorstr='#ffededed', GradientType=0);
  border-bottom: 1px solid #c7c7c7;
  padding-top: 1px;
}
#dashboard_dropdown { position: absolute;right: 0;z-index: 2;padding: 0 8px 0 0; width: 115px; top: 0px;}

/************ Tabs ************/

ul#searchtoolbar  { margin: 0;padding: 2px 0 0 5px; width:100%;height: 30px; background: url(/images/layout/teaserbarlg3.png) left bottom repeat-x;}
